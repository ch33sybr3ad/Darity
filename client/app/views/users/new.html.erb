<div class= "Sign Up">
    <h1>Sign Up</h1>
      <%= form_for(@user) do |f| %>

      <%= f.label :username %>
        <%= f.text_field :username, class: 'form-control' %>

      <%= f.label :email %>
        <%= f.email_field :email, class: 'form-control' %>

        <%= f.label :password %>
        <%= f.password_field :password, class: 'form-control' %>

        <%= f.submit "Sign Up", class: "btn btn-primary" %>
    <% end %>
</div>
